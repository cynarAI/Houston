# Database
DATABASE_URL=mysql://user:password@host:port/database

# Authentication (Manus OAuth)
JWT_SECRET=your-jwt-secret-here
VITE_APP_ID=your-manus-app-id
OAUTH_SERVER_URL=https://api.manus.im
OWNER_OPEN_ID=your-owner-open-id

# Manus LLM API
BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=your-manus-api-key
HOUSTON_EMBEDDED_MODEL=gemini-2.5-flash
HOUSTON_TTS_MODEL=gpt-4o-mini-tts
HOUSTON_STT_MODEL=whisper-1
EMBEDDED_TIMEOUT_MS=120000

# Manus API (fallback/auto)
MANUS_API_BASE=https://api.manus.ai
MANUS_API_KEY=your-manus-api-key
MANUS_API_TIMEOUT_MS=120000
HOUSTON_AI_PROVIDER=embedded # embedded|manus_api|auto|mock (mock f√ºr lokale Tests)
HOUSTON_AI_FALLBACK=true

# Stripe Payment Integration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_...

# Sentry Error Tracking (optional)
SENTRY_DSN=https://xxx@sentry.io/xxx
VITE_SENTRY_DSN=https://xxx@sentry.io/xxx
# VITE_SENTRY_DEBUG=true  # Enable in dev

# Plausible Analytics (optional)
VITE_PLAUSIBLE_DOMAIN=houston.manus.space
# VITE_PLAUSIBLE_DEBUG=true  # Enable in dev

# Node Environment
NODE_ENV=development

# Development Testing (optional)
# Set to 'true' to bypass OAuth and use a mock user for visual testing
# DEV_MOCK_AUTH=true
